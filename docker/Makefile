.PHONY: build push aws

#######################################################################
# Build
#######################################################################
build:
	cd aws; DOCKER_TLS_VERIFY= DOCKER_HOST=unix:///var/run/docker.sock docker-compose build

#######################################################################
# Push
#######################################################################
push:
	./push.sh

#######################################################################
# Run
#######################################################################
aws:
	cd aws; docker-compose up -d
